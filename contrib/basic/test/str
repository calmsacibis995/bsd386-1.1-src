  100	dim a$256, b$, c$45
  110	a$ = '012345678911234567892123456789312345678941234567895123456789'
  120	a$ = a$ || '6123456789712345678981234567899123456789a123456789b123456789'
  130	a$ = a$ || 'c123456789d123456789e123456789f123456789g123456789h123456789'
  140	b$ = a$
  150	c$ = a$
  160	print 'length of a$ is '; len(a$)
  170	print 'length of b$ is '; len(b$)
  180	print 'length of c$ is '; len(c$)
  190	print 'b$ = '; b$; '.  c$ = '; c$
  200	print 'a$ = '; a$
  210	end
